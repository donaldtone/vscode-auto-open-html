	"description": "auto open html preview",
	"publisher": "Donaldtone",
	"author": "Donald Tone",
	"license": "MIT",
	"readme": "README.md",
	"version": "1.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/donaldtone/vscode-auto-open-html.git"
	},
	"homepage": "https://github.com/donaldtone/vscode-auto-open-html",
